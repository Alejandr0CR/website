  <NavBarTop theme="dark" active="about" />

  <header class="container mb-5">
    <section class="no-selectable bg-black text-light rounded-4">
			<div class="py-3">
				<h1 class="h4 d-block text-center animate-name">Acerca de mí</h1>
			</div>
		</section>
  </header>

  <Footer />
</Layout>